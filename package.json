{
  "name": "winsleague",
  "version": "2.0.0",
  "description": "Wins League",
  "readme": "README",
  "repository": {
    "type": "git",
    "url": "https://github.com/winsleague/winsleague.git"
  },
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "babel-runtime": "6.26.0",
    "bcrypt": "1.0.3",
    "faker": "4.1.0",
    "lodash": "4.17.5",
    "loggly": "1.1.1",
    "meteor-node-stubs": "0.3.3",
    "moment": "2.22.1",
    "moment-timezone": "0.5.14",
    "simpl-schema": "1.4.3",
    "store": "2.0.12",
    "winston": "2.3.0",
    "winston-loggly-bulk": "1.3.4"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "chai-jquery": "2.0.0",
    "chimp": "0.51.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-meteor": "^5.0.0",
    "eslint-plugin-react": "^7.5.1",
    "selenium-webdriver": "^3.6.0",
    "shell-source": "1.1.0",
    "sinon": "~4.4.5",
    "sinon-chai": "~3.0.0",
    "xmlbuilder": "9.0.7"
  },
  "greenkeeper": {
    "ignore": [
      "babel-runtime",
      "loggly",
      "winston",
      "winston-loggly-bulk"
    ]
  },
  "scripts": {
    "start": "meteor",
    "pretest": "echo 'npm run lint --silent'",
    "test": "TEST_BROWSER_DRIVER=chrome meteor test --once --driver-package meteortesting:mocha --port 3100",
    "test-app": "TEST_BROWSER_DRIVER=chrome meteor test --full-app --once --driver-package meteortesting:mocha --port 3100",
    "test-watch": "meteor test --driver-package meteortesting:mocha --port 3100",
    "test-app-watch": "meteor test --full-app --driver-package meteortesting:mocha --port 3100",
    "test-watch-terminal": "TEST_WATCH=1 TEST_BROWSER_DRIVER=chrome meteor test --driver-package meteortesting:mocha --port 3100",
    "test-app-watch-terminal": "TEST_WATCH=1 TEST_BROWSER_DRIVER=chrome meteor test --full-app --driver-package meteortesting:mocha --port 3100",
    "lint": "eslint ."
  },
  "author": "",
  "license": "GPL-2.0"
}
